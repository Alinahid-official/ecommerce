<%- include('partials/header')%>
<div class="col-md-8">
    <% products.forEach(function(p) { %>
    <div class="product_list">
        <div class="row">
            <div class="col-lg-6 col-sm-6">
                <div class="single_product_item">
                    <img src="/product-images/<%= p.id %>/<%= p.image %>" alt="" class="img-fluid">
                    <h3> <a href="/products/<%= p.category %>/<%= p.slug%>">Cervical pillow for airplane
                        <%= p.title %></a> </h3>
                    <p>From <%= parseFloat(p.price).toFixed(2) %></p>
                </div>
            </div>
        </div>
    </div>
    <% }); %>
</div>

<%- include('partials/footer') %>